{
  "name": "parcel-sample",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run serve\" \"npm run server\"",
    "serve": "parcel serve ./src/index.html --port 1234",
    "build": "parcel build ./src/index.html",
    "server": "node ./server.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "jsrsasign": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "parcel": "^2.12.0"
  }
}
